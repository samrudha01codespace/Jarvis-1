# Directory to save the downloaded songs
download_dir = "downloaded_songs_marathi"
os.makedirs(download_dir, exist_ok=True)

def download_song(song_name):
    search = Search(song_name)
    results = search.results
    if results:
        video = results[0]
        print(f"Downloading {song_name}...")
        video.streams.filter(only_audio=True).first().download(output_path=download_dir)
        print(f"Downloaded: {song_name}")
    else:
        print(f"Song not found: {song_name}")

for song in songs:
    download_song(song)

print("All songs downloaded.")